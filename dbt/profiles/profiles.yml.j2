config:
  log_format: default
  use_colors: false

pokemon:
  target: dev
  outputs:
    prod:
      dataset: output_data
      job_execution_timeout_seconds: 600
      job_retries: 1
      location: EU
      method: service-account
      priority: interactive
      project: {{ PROJECT_ID }}
      threads: 1
      type: bigquery
      keyfile: credentials.json
    dev:
      dataset: output_data
      job_execution_timeout_seconds: 600
      job_retries: 1
      location: EU
      method: oauth
      priority: interactive
      project: {{ PROJECT_ID }}
      threads: 1
      type: bigquery